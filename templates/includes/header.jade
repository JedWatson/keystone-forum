header(role='banner').docs-header
	.container
		button.navbar-toggle.navbar-toggle-menu(type='button', data-toggle='collapse', data-target='.docs-nav__collapse')
			span.sr-only Toggle navigation
			//- span.entypo.entypo-list
			!='<span class="icon-bar"></span><span class="icon-bar"></span><span class="icon-bar"></span>'
		a(href='/', title='KeystoneJS').keystone-logo: img(src='/images/logo.svg', alt='KeystoneJS', width=207, height=70)
		
		nav.docs-nav
			.docs-nav__collapse.collapse
				ul.docs-nav__nav
					if current.user
						li: a(href=js) Ask question
					else
						li: a(href='/signin') Log in
		//- include ../includes/primary-nav
